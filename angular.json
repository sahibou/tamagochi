{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"tamagochi": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/tamagochi",
						"index": "src/index.html",
						"browser": "src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"src/styles.scss",
							"@angular/material/prebuilt-themes/azure-blue.css",
							"node_modules/leaflet/dist/leaflet.css"
						],
						"scripts": [],
						"server": "src/main.server.ts",
						"outputMode": "server",
						"ssr": {
							"entry": "src/server.ts"
						}
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "tamagochi:build:production"
						},
						"development": {
							"buildTarget": "tamagochi:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"@angular/material/prebuilt-themes/azure-blue.css",
							"src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
		"ign-geos-completion-swagger-client": {
			"projectType": "library",
			"root": "projects/ign-geos-completion-swagger-client",
			"sourceRoot": "projects/ign-geos-completion-swagger-client/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/ign-geos-completion-swagger-client/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/ign-geos-completion-swagger-client/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/ign-geos-completion-swagger-client/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"tsConfig": "projects/ign-geos-completion-swagger-client/tsconfig.spec.json",
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						]
					}
				}
			}
    },
    "api-cadastre-parcelle": {
      "projectType": "library",
      "root": "projects/api-cadastre-parcelle",
      "sourceRoot": "projects/api-cadastre-parcelle/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/api-cadastre-parcelle/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/api-cadastre-parcelle/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/api-cadastre-parcelle/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/api-cadastre-parcelle/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
	}
}